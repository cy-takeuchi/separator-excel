{
    "name": "ochi51/separator-excel",
    "description": "Separate excel with images for cli-kintone",
    "type": "library",
    "keywords": ["Excel", "Kintone"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuichi Ochiai",
            "email": "ochiai07@gmail.com",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=7.2",
        "phpoffice/phpspreadsheet": "^1.14",
        "symfony/console": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Ochi51\\SeparatorExcel\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Ochi51\\SeparatorExcel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": [
        "bin/separator-excel"
    ]
}
